{
  "name": "picocolors",
  "version": "0.0.1",
  "type": "module",
  "main": "./build/picocolors.cjs",
  "module": "picocolors.js",
  "exports": {
    ".": {
      "require": "./build/picocolors.cjs",
      "import": "./picocolors.js",
      "default": "./picocolors.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "description": "The tiniest and the fastest coloring library ever",
  "scripts": {
    "build:cjs": "tsc picocolors.js --allowJs --module commonjs --target es5 --outDir build && mv ./build/picocolors.js ./build/picocolors.cjs",
    "test": "node test.js"
  },
  "files": [
    "picocolors.d.ts",
    "picocolors.js.flow"
  ],
  "keywords": [
    "terminal",
    "colors",
    "formatting",
    "cli",
    "console"
  ],
  "author": "Alexey Raspopov",
  "repository": "alexeyraspopov/picocolors",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.4.1"
  },
  "prettier": {
    "printWidth": 100
  }
}
